{
  "name": "@vantezzen/pow",
  "description": "A simple challenge-response proof-of-work implementation for web apps",
  "license": "MIT",
  "keywords": [
    "pow",
    "proof-of-work",
    "challenge-response",
    "challenge",
    "response",
    "web",
    "webcrypto",
    "webworker",
    "worker",
    "vite",
    "esm",
    "typescript",
    "javascript",
    "crypto",
    "cryptography",
    "hashing",
    "hash",
    "browser"
  ],
  "version": "1.0.1",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/pow.umd.cjs",
  "module": "./dist/pow.js",
  "exports": {
    ".": {
      "import": "./dist/pow.js",
      "require": "./dist/pow.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:server": "node --experimental-specifier-resolution=node --loader ts-node/esm demo/index.ts",
    "build": "tsc && vite build",
    "build:client": "tsc && vite build --config vite.client.config.js",
    "preview": "vite preview"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  },
  "dependencies": {
    "@peculiar/webcrypto": "^1.4.1"
  }
}
